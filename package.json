{
  "name": "aqie-dc-backend",
  "version": "0.0.0",
  "description": "aqie-dc-backend",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm run server:watch",
    "dev:debug": "cross-env NODE_ENV=development npm run server:debug",
    "build": "npm run build:server",
    "build:server": "babel --delete-dir-on-start --ignore \"**/*.test.js\" --ignore \"src/__fixtures__\" --copy-files --no-copy-ignored --out-dir ./.server ./src",
    "docker:dev": "npm run dev",
    "format": "prettier --write \"**/*.{cjs,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{cjs,js,json,md}\"",
    "git:pre-commit-hook": "npm run security-audit && npm run format:check && npm run lint && npm test",
    "lint": "eslint --cache --cache-strategy content \"**/*.{cjs,js}\"",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "npm run setup:husky",
    "security-audit": "npm audit --audit-level=high",
    "server:debug": "nodemon --inspect-brk=0.0.0.0 --ext js,json --legacy-watch ./src",
    "server:watch": "nodemon --inspect=0.0.0.0 --ext js,json --legacy-watch ./src",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module",
    "start": "cross-env NODE_ENV=production node --use-strict .",
    "test": "cross-env TZ=UTC vitest run --coverage",
    "test:watch": "cross-env TZ=UTC vitest",
    "import:appliances": "node src/migrations/import-from-excel.js --type appliances --file",
    "import:fuels": "node src/migrations/import-from-excel.js --type fuels --file",
    "import:both": "node src/migrations/import-from-excel.js --type both --file",
    "import:generate-templates": "node src/migrations/generate-excel-template.js",
    "import:test": "node src/migrations/test-import.js"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@aws-sdk/client-s3": "3.978.0",
    "@aws-sdk/credential-providers": "3.978.0",
    "@babel/runtime": "7.28.6",
    "@defra/cdp-auditing": "0.6.0",
    "@defra/hapi-secure-context": "0.4.0",
    "@defra/hapi-tracing": "1.30.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/boom": "10.0.1",
    "@hapi/hapi": "21.4.4",
    "@hapi/inert": "7.1.0",
    "aws-embedded-metrics": "4.2.1",
    "aws4": "1.13.2",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "date-fns": "4.1.0",
    "global-agent": "3.0.0",
    "hapi-pino": "13.0.0",
    "hapi-pulse": "3.0.1",
    "https-proxy-agent": "7.0.6",
    "joi": "18.0.2",
    "mongo-locks": "3.1.2",
    "mongodb": "7.0.0",
    "node-fetch": "3.3.2",
    "pino": "10.1.0",
    "pino-pretty": "13.1.3",
    "undici": "7.19.2",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@babel/cli": "7.27.2",
    "@babel/core": "7.27.4",
    "@babel/node": "7.23.9",
    "@babel/preset-env": "7.28.5",
    "@vitest/coverage-v8": "4.0.16",
    "autoprefixer": "10.4.23",
    "cross-env": "10.0.0",
    "eslint": "9.39.2",
    "husky": "9.1.7",
    "neostandard": "0.12.2",
    "nodemon": "3.1.11",
    "npm-run-all": "4.1.5",
    "prettier": "3.7.4",
    "vitest": "4.0.16",
    "vitest-fetch-mock": "0.4.5",
    "vitest-mongodb": "1.0.3"
  }
}
